{
  "name": "contextmanager",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build --watch",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "ts-jest"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ]
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "idb": "^8.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "watch": "^0.13.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/chrome": "^0.0.317",
    "@types/jest": "^30.0.0",
    "@types/mocha": "^10.0.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "fake-indexeddb": "^6.0.1",
    "globals": "^16.0.0",
    "jest": "^30.0.3",
    "tailwindcss": "^4.1.8",
    "ts-jest": "^29.4.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
